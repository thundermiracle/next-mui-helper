{
  "presets": [
    "next/babel",
    "env",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["/"],
      "alias": {
        "src": "./src"
      }
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  },
  "ignore": "node_modules/**"
}