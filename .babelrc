{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "react-require",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src/"
        ],
        "alias": {
          "material-ui": "@material-ui/core",
          "src": "./src"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  },
  "ignore": [
    "node_modules/**"
  ]
}